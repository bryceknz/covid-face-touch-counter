{
  "expo": {
    "name": "Face Touch Counter",
    "slug": "covid-face-touch-counter",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.bryceknz.covidfacetouchcounter",
      "versionCode": 1
    },
    "description": "A way to track the number of times you touch your face each day.",
    "githubUrl": "https://github.com/bryceknz/covid-face-touch-counter"
  }
}
